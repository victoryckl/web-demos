        <url-pattern>/cache</url-pattern>
    </servlet-mapping>
	
	<context-param>
		<param-name>sname</param-name>
		<param-value>here is web.xml</param-value>
	</context-param>
	<context-param>
		<param-name>zzzzzz</param-name>
		<param-value>whowhowho</param-value>
	</context-param>
	
    <servlet>
